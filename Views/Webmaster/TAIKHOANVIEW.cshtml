@model IEnumerable<NATURALLIFE.Models.TAIKHOAN>

@{
    ViewBag.Title = "TAIKHOANVIEWVIEW";
    Layout = "~/Views/Shared/_Webmaster.cshtml";
}
<section id="main-content">
    <section class="wrapper">
        <!-- BASIC FORM ELELEMNTS -->
        <h3><i class="fa fa-angle-right"></i>TÀI KHOẢN ĐĂNG NHẬP</h3>
        <!-- row -->
        <!-- /row -->
        <div class="col-lg-12">
            <div class="content-panel">
                <section id="unseen">
                    <table class="table table-bordered table-striped table-condensed">
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <button class="btn btn-success btn-primary">
                                    <i class="fa fa-check"></i>
                                    @Html.ActionLink("EDIT", "TAIKHOANEDIT", new { id = item.IDTAIKHOAN })
                                </button>
                                <tr>
                                    <td class="col-lg-2">TÀI KHOẢN</td>
                                    <td class="col-lg-10">
                                        <p>ADMIN</p>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="col-lg-2">MẬT KHẨU</td>
                                    <td class="col-lg-10">
                                        <p> @Html.DisplayFor(modelItem => item.PASS)</p>
                                    </td>
                                </tr>

                            }
                        </tbody>
                    </table>
                </section>
            </div>
            <!-- /content-panel -->
        </div>
        <!-- /row -->
        <!-- /row -->
    </section>
    <!-- /wrapper -->
</section>
