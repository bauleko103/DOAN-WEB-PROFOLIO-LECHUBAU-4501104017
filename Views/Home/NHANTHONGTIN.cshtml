@model NATURALLIFE.Models.DSKHACHHANG







<header class="init">
    <!-- subnav -->
    <div class="subnav">
        <div class="container">
            <div class="row">

                @Html.Action("LINKPAGETOP")

            </div>
        </div>
    </div>
    <!-- subnav end -->
    <!-- nav -->
    <div class="navbar-default-white navbar-fixed-top">
        <!-- container -->
        <div class="container">
            <div class="row">

                <!-- menu mobile display -->
                <button class="navbar-toggle" data-target=".navbar-collapse" data-toggle="collapse">
                    <span class="icon icon-bar"></span>
                    <span class="icon icon-bar"></span>
                    <span class="icon icon-bar"></span>
                </button>

                <!-- logo -->
                <a class="navbar-brand white" href="@Url.Action("Index","Home")">
                    <img class="white" alt="logo" src="~/Content/img/LOGOBIG-01.png">
                    <img class="black" alt="logo" src="~/Content/img/LOGOBIG-01.png">
                </a>
                <!-- logo end -->
                <!-- mainmenu start -->
                <div class="white menu-init" id="main-menu">
                    <nav id="menu-center">
                        <ul>
                            <li>
                                <a href="@Url.Action("Index","Home")#home">Home</a>
                            </li>
                            <!-- <li>
      <a href="#about-us-1">About</a>
    </li> -->
                            <li>
                                <a href="@Url.Action("Index","Home")#service">MODEL</a>
                            </li>
                            <li>
                                <a href="@Url.Action("Index","Home")#projects">GALLARY</a>
                            </li>
                            <li>
                                <a href="@Url.Action("Index","Home")#blog">BLOG</a>
                            </li>
                            <li>
                                <a href="@Url.Action("Index","Home")#contact">CONTACT</a>
                            </li>
                            <li>
                                <a class="btn-book-menu popup-form actived" href="@Url.Action("NHANTHONGTIN","Home")">BOOK NOW</a>
                            </li>
                            
                        </ul>
                    </nav>
                </div>
                <!-- mainmenu end -->

            </div>
        </div>
        <!-- container end-->
    </div>
    <!-- nav end -->
</header>
<!-- subheader -->
<section id="subheader">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h1>
                    @ViewBag.Title
                </h1>
            </div>

            <!-- devider -->
            <div class="col-md-12">
                <div class="devider-page">
                    <div class="devider-img-right">
                    </div>
                </div>
            </div>

            <div class="col-md-12">
                <ul class="subdetail">
                    <li>
                        <a href="@Url.Action("Index","Home")#home">Home</a>
                    </li>

                    <li class="sep">
                        /
                    </li>

                    <li>
                       BOOK NOW
                    </li>
                </ul>
            </div>
        </div>
    </div>
</section>
<!-- subheader end -->
<!-- section -->
<section aria-label="section-blogg" id="content">
    <div class="container">
        <div class="row">

            <!-- left content -->
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()


                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                <div class="onStep" data-animation="fadeInUp" data-time="300" id="contact">
                    <form action="#" class="row" id="form-contact" method="post" name="form-contact">

                        <div class="col-lg-12">
                            <div class="col-lg-1">
                                <p>NAME</p>
                            </div>
                            <div class="col-lg-11">
                                <p>
                                    @Html.EditorFor(model => model.ID, new { htmlAttributes = new { @class = "form-control" } })
                                </p>
                            </div>
                        </div>
                        <div class="col-lg-12">
                            <div class="col-lg-1">
                                <p>GMAIL</p>
                            </div>
                            <div class="col-lg-11">
                                <p>
                                    @Html.EditorFor(model => model.GMAIL, new { htmlAttributes = new { @class = "form-control" } })
                                </p>
                            </div>
                        </div>

                        <textarea cols="50" id="message-contact" name="message" placeholder="your enquiry" rows="4"></textarea>
                        <div class="success" id="mail_success">
                            Thank you. Your message has been sent
                        </div>

                        <div class="error" id="mail_failed">
                            Error, email not sent
                        </div>
                    </form>
                    <button type="submit" class="btn-contact" id="send-contact">
                        <span>BOOK</span>
                    </button>
                </div>

            }
            <!-- left content end -->
            <!-- right content -->
           <!-- right content end -->
        </div>
    </div>
</section>
<!-- section -->
<script src="~/ContentWebmaster/css/DATETIME.js"></script>