<!DOCTYPE html>
<html lang="en-us">

<head>
    <meta charset="utf-8">
    <title>
        @ViewBag.Title
    </title>
    <meta content="" name="description">
    <meta content="" name="author">
    <meta content="" name="keywords">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1" name="viewport">
    <!-- favicon -->
    <link href="~/Content/img/favicon.png" rel="icon" sizes="32x32" type="image/png">
    <!-- Bootstrap CSS -->
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/css/jquery.dateselect.css" rel="stylesheet">
    <!-- font themify CSS -->
    <link href="~/Content/css/themify-icons.css" rel="stylesheet">
    <!-- font awesome CSS -->
    <link href="~/Content/font-awesome/css/font-awesome.css" rel="stylesheet">
    <!-- on3step CSS -->
    <link href="~/Content/css/animated-on3step.css" rel="stylesheet">
    <link href="~/Content/css/owl.carousel.css" rel="stylesheet">
    <link href="~/Content/css/owl.theme.css" rel="stylesheet">
    <link href="~/Content/css/owl.transitions.css" rel="stylesheet">
    <link href="~/Content/css/on3step-style.css" rel="stylesheet" />
    <link href="~/Content/css/queries-on3step.css" media="all" rel="stylesheet" type="text/css">



</head>

<body>
    <!-- preloader -->
    <div class="bg-preloader-white">

    </div>
    <div class="preloader-white">
        <div class="mainpreloader">
            <span></span>
        </div>
    </div>
    <!-- preloader end -->
    <!-- content wraper -->
    <div class="content-wrapper">

        @RenderBody();

        @Html.Action("Footer");

        <!-- ScrolltoTop -->
        <div id="totop">
            <span class="ti-angle-up"></span>
        </div>
    </div>
    <!-- content wraper end -->
    <!-- plugin JS -->
    <script src="~/Content/plugin/pluginson3step.js" type="text/javascript"></script>
    <script src="~/Content/plugin/parallax.min.js" type="text/javascript"></script>
    <script src="~/Content/plugin/sticky.js" type="text/javascript"></script>
    <script src="~/Content/js/jquery.dateselect.js" type="text/javascript"></script>
    <!-- slider revolution  -->
    <script src="~/Content/rs-plugin/js/jquery.themepunch.revolution.min.js" type="text/javascript"></script>
    <!-- on3step JS -->
    <script src="~/Content/js/on3step.js" type="text/javascript"></script>
    <script src="~/Content/js/plugin-set.js" type="text/javascript"></script>
    <script src="~/Content/js/nav.js" type="text/javascript"></script>
    <script src="~/ckeditor_4.14.0_full_easyimage/ckeditor/ckeditor.js"></script>

    
</body>
</html>

