<!DOCTYPE html>
<html lang="en">

<head class="header black-bg">
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Dashboard">
    <meta name="keyword" content="">
    <title>@ViewBag.Title</title>

    <!-- Favicons -->
    <link href="~/Content/img/favicon.png" rel="icon" sizes="32x32" type="image/png">
    <link href="~/Content/img/favicon.png" rel="icon" sizes="32x32" type="image/png">

    <!-- Bootstrap core CSS -->
    <link href="~/ContentWebmaster/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!--external css-->
    <link href="~/ContentWebmaster/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="~/ContentWebmaster/lib/bootstrap-datepicker/css/datepicker.css" />
    <link rel="stylesheet" type="text/css" href="~/ContentWebmaster/lib/bootstrap-daterangepicker/daterangepicker.css" />
    <!-- Custom styles for this template -->
    <link href="~/ContentWebmaster/css/style.css" rel="stylesheet">
    <link href="~/ContentWebmaster/css/style-responsive.css" rel="stylesheet">


    <!-- CSS to style the file input field as button and adjust the Bootstrap progress bars -->
    <link rel="stylesheet" href="~/ContentWebmaster/lib/file-uploader/css/jquery.fileupload.css">
    <link rel="stylesheet" href="~/ContentWebmaster/lib/file-uploader/css/jquery.fileupload-ui.css">
    <!-- CSS adjustments for browsers with JavaScript disabled -->
    <noscript>
        <link rel="stylesheet" href="~/ContentWebmaster/lib/file-uploader/css/jquery.fileupload-noscript.css">
    </noscript>
    <noscript>
        <link rel="stylesheet" href="~/ContentWebmaster/lib/file-uploader/css/jquery.fileupload-ui-noscript.css">
    </noscript>


</head>

<body>
    <header class="header black-bg">
        <div class="sidebar-toggle-box">
            <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
        </div>
        <!--logo start-->
        <a href="@Url.Action("HOMEVIEW")" class="logo"><b>BAU<span>LEE</span></b></a>
        <!--logo end-->

        <div class="top-menu">
            <ul class="nav pull-right top-menu">
                <li>
                    <a class="logout" href="@Url.Action("Login", "Webmaster")">Logout</a>
                </li>
            </ul>
        </div>
    </header>
    <section id="container">
        <!-- **********************************************************************************************************************************************************
        TOP BAR CONTENT & NOTIFICATIONS
        *********************************************************************************************************************************************************** -->
        <!--header end-->
        <!-- **********************************************************************************************************************************************************
        MAIN SIDEBAR MENU
        *********************************************************************************************************************************************************** -->
        <!--sidebar start-->
        <aside>
            <div id="sidebar" class="nav-collapse ">
                <!-- sidebar menu start-->
                <ul class="sidebar-menu" id="nav-accordion">
                    <p class="centered">
                        <a href="profile.html"></a>
                    </p>
                    <h5 class="centered">ADMIN</h5>

                    <li class="mt">
                        <a href="@Url.Action("HOMEVIEW")">
                            <i class="fa fa-dashboard"></i>
                            <span>HOME</span>
                        </a>
                    </li>
                    <li class="sub-menu">
                        <a href="@Url.Action("MODELVIEW")">
                            <i class="fa fa-archive"></i>
                            <span>MODEL</span>
                        </a>
                    </li>
                    <li class="sub-menu">
                        <a href="javascript:;">
                            <i class="fa fa-desktop"></i>
                            <span>GALLARY</span>
                        </a>
                        <ul class="sub">
                            <li><a class="fa fa-caret-right" href="@Url.Action("THUVIENVIEW")"> GALLARY PAGE</a></li>
                            <li><a class="fa fa-caret-right" href="@Url.Action("THELOAITHUVIENVIEW")"> CATEGORIES GALLARY</a></li>

                        </ul>
                    </li>

                    <li class="sub-menu">
                        <a href="javascript:;">
                            <i class="fa fa-envelope"></i>
                            <span>BLOGS</span>
                        </a>
                        <ul class="sub">
                            <li>
                                <a class="fa fa-caret-right" href="@Url.Action("TINTUCVIEW")"> BLOG PAGE</a>
                            </li>
                            <li>
                                <a class="fa fa-caret-right" href="@Url.Action("THELOAITINTUCVIEW")"> CATEGORIES BLOG</a>
                            </li>
                        </ul>
                    </li>
                    <li class="sub-menu">
                        <a href="javascript:;">
                            <i class="fa fa-phone"></i>
                            <span>CONTACT</span>
                        </a>
                        <ul class="sub">
                            <li>
                                <a class="fa fa-caret-right" href="@Url.Action("CONTACTVIEW")"> CONTACT PAGE</a>
                            </li>
                            <li>
                                <a class="fa fa-caret-right" href="@Url.Action("LISTCONTACTVIEW")">bLIST OF CUSTOMER</a>
                            </li>
                        </ul>
                    </li>
                    <li class="sub-menu">
                        <a href="@Url.Action("CLIENTVIEW")">
                            <i class="fa fa-lemon-o"></i>
                            <span>CLIENT</span>
                        </a>
                    </li>
                    <li class="sub-menu">
                        <a href="@Url.Action("FOOTVIEW")">
                            <i class="fa fa-rocket"></i>
                            <span>FOOT</span>
                        </a>
                    </li>
                    <li class="sub-menu">
                        <a href="@Url.Action("PHOTOVIEW")">
                            <i class="fa fa-camera-retro"></i>
                            <span>PHOTO</span>
                        </a>
                    </li>
                    <li class="sub-menu">
                        <a href="@Url.Action("SUBBODYVIEW")">
                            <i class="fa fa-umbrella"></i>
                            <span>LINK PAGE</span>
                        </a>
                    </li>
                    <li class="sub-menu">
                        <a href="@Url.Action("TAIKHOANVIEW")">
                            <i class="fa fa-user"></i>
                            <span>ACCOUNT</span>
                        </a>
                    </li>

                </ul>
                <!-- sidebar menu end-->
            </div>
        </aside>
    </section>
    @RenderBody()
    <!-- js placed at the end of the document so the pages load faster -->
    <script src="~/ContentWebmaster/lib/jquery/jquery.min.js"></script>
    <script src="~/ContentWebmaster/lib/bootstrap/js/bootstrap.min.js"></script>
    <script class="include" type="text/javascript" src="~/ContentWebmaster/lib/jquery.dcjqaccordion.2.7.js"></script>
    <script src="~/ContentWebmaster/lib/jquery.scrollTo.min.js"></script>
    <script src="~/ContentWebmaster/lib/jquery.nicescroll.js" type="text/javascript"></script>
    <!--common script for all pages-->
    <script src="~/ContentWebmaster/lib/common-scripts.js"></script>
    <!--script for this page-->
    <script src="~/ContentWebmaster/lib/jquery-ui-1.9.2.custom.min.js"></script>
    <!--custom switch-->
    <script src="~/ContentWebmaster/lib/bootstrap-switch.js"></script>
    <!--custom tagsinput-->
    <script src="~/ContentWebmaster/lib/jquery.tagsinput.js"></script>
    <!--Contactform Validation-->
    <script src="~/ContentWebmaster/lib/php-mail-form/validate.js"></script>
    <script src="~/ContentWebmaster/ckeditor/ckeditor.js"></script>
</body>

</html>

